{% extends '::base-admin.html.twig' %}

{% block main %}
    <h1>Droits</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Droitsacquisanneeencours</th>
                <td>{{ entity.droitsAcquisAnneeEnCours }}</td>
            </tr>
            <tr>
                <th>Reliquatdroitsanterieur</th>
